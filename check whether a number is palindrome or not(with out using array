#include<stdio.h>

int main()
{
    int num,temp,revnum;
    scanf("%d",&num);
    temp=num;
    while(num)
    {
        int digit=num%10;
        revnum=revnum*10 + digit;
        num=num/10;
        
    }
    if(revnum==temp)
    printf("Entered number is a palindrome");
    else
     printf("Entered number is not a palindrome");
}
